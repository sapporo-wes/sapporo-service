{
  "realm": "sapporo-dev",
  "enabled": true,
  "sslRequired": "none",
  "accessTokenLifespan": 3600,
  "ssoSessionIdleTimeout": 28800,
  "ssoSessionMaxLifespan": 86400,
  "registrationAllowed": false,
  "clients": [
    {
      "clientId": "sapporo-service-dev",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "redirectUris": ["*"],
      "webOrigins": ["*"],
      "defaultClientScopes": [
        "basic",
        "profile",
        "email"
      ],
      "protocolMappers": [
        {
          "name": "audience-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "config": {
            "included.client.audience": "sapporo-service-dev",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ]
    },
    {
      "clientId": "sapporo-service-dev-confidential",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "sapporo-dev-client-secret",
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "serviceAccountsEnabled": true,
      "redirectUris": ["*"],
      "webOrigins": ["*"],
      "defaultClientScopes": [
        "basic",
        "profile",
        "email"
      ],
      "protocolMappers": [
        {
          "name": "audience-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "config": {
            "included.client.audience": "sapporo-service-dev-confidential",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "test-user",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Test",
      "lastName": "User",
      "email": "test-user@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "test-user-password",
          "temporary": false
        }
      ]
    },
    {
      "username": "test-user-2",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Test2",
      "lastName": "User2",
      "email": "test-user-2@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "test-user-2-password",
          "temporary": false
        }
      ]
    }
  ]
}
